<p id="notice"><%= notice %></p>
<center>
<div class="projects-container">
  <div class="col-md-12">
      <h1>Projects</h1>
      <hr>
    <table>
      <thead>
        <tr>
          <th>Title |</th>
          <th>Start date |</th>
          <th>End date |</th>
          <th>Project type |</th>
          <th>Role |</th>
          <th>Key skill</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @projects.each do |project| %>
          <tr>
            <td><%= project.title %> </td>
            <td><%= project.start_date %></td>
            <td><%= project.end_date %></td>
            <td><%= project.project_type %></td>
            <td><%= project.role %></td>
            <td><%= project.key_skill %></td>
            <td><%= link_to 'Show', project %>|</td>
            <td><%= link_to 'Edit', edit_project_path(project) %>|</td>
            <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>
  </div>
</div>

<%= link_to 'New Project', new_project_path %></center>
